{
  "name": "web",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next node_modules .turbo",
    "dev": "next dev --port 3001",
    "build": "next build && postcss-dedup .next/static/chunks && cp -r public .next/standalone/apps/web/public && cp -r .next/static .next/standalone/apps/web/.next/static",
    "start": "node .next/standalone/apps/web/server.js",
    "check-types": "next typegen && tsc --noEmit"
  },
  "dependencies": {
    "@repo/feature-a": "workspace:*",
    "@repo/feature-b": "workspace:*",
    "@repo/ui": "workspace:*",
    "next": "16.1.5",
    "react": "^19.2.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@repo/postcss-dedup": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.5",
    "typescript": "5.9.2"
  }
}
